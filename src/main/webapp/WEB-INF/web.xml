<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>famtree</display-name>
  <servlet>
  	<servlet-name>IndexServlet</servlet-name>
  	<display-name>IndexServlet</display-name>
  	<description></description>
  	<servlet-class>servlet.IndexServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AliveServlet</servlet-name>
  	<display-name>AliveServlet</display-name>
  	<description></description>
  	<servlet-class>servlet.AliveServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DiedServlet</servlet-name>
  	<display-name>DiedServlet</display-name>
  	<description></description>
  	<servlet-class>servlet.DiedServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>IndexServlet</servlet-name>
  	<url-pattern>/</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AliveServlet</servlet-name>
  	<url-pattern>/alive</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DiedServlet</servlet-name>
  	<url-pattern>/died</url-pattern>
  </servlet-mapping>
</web-app>
