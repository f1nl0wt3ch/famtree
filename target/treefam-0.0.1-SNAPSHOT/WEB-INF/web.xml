<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >
<web-app>
  <servlet>
  	<servlet-name>IndexServlet</servlet-name>
  	<display-name>IndexServlet</display-name>
  	<description></description>
  	<servlet-class>servlet.IndexServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AliveServlet</servlet-name>
  	<display-name>AliveServlet</display-name>
  	<description></description>
  	<servlet-class>servlet.AliveServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DiedServlet</servlet-name>
  	<display-name>DiedServlet</display-name>
  	<description></description>
  	<servlet-class>servlet.DiedServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DeleteServlet</servlet-name>
  	<display-name>DeleteServlet</display-name>
  	<description></description>
  	<servlet-class>servlet.DeleteServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>QuarztServlet</servlet-name>
  	<display-name>QuarztServlet</display-name>
  	<description></description>
  	<servlet-class>servlet.QuarztServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UpdateServlet</servlet-name>
  	<display-name>UpdateServlet</display-name>
  	<description></description>
  	<servlet-class>servlet.UpdateServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>IndexServlet</servlet-name>
  	<url-pattern>/famtree</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AliveServlet</servlet-name>
  	<url-pattern>/alive</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DiedServlet</servlet-name>
  	<url-pattern>/died</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeleteServlet</servlet-name>
  	<url-pattern>/delete</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>QuarztServlet</servlet-name>
  	<url-pattern>/QuarztServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UpdateServlet</servlet-name>
  	<url-pattern>/update</url-pattern>
  </servlet-mapping>
</web-app>
